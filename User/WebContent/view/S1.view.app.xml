<core:View xmlns:core="sap.ui.core"
	xmlns="sap.m" controllerName="User.view.S1">

	<Page id="page">
		<content>
			<!-- Replace with required full screen control -->
			<List id="list" items="{/Users}" mode="{device>/listMode}" select="_handleSelect">
				<ObjectListItem id="MAIN_LIST_ITEM" type="{device>/listItemType}" press="_handleItemSelect"
					title="{Firstname}"
					number="{}"
					numberUnit="{}">
					<attributes>
						<ObjectAttribute id="ATTR1" text="{Firstname}" />
						<ObjectAttribute id="ATTR2" text="{Lastname}" />
					</attributes>
				</ObjectListItem>
			</List>
		</content>
	</Page>
</core:View>