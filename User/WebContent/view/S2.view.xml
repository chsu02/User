<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<core:View xmlns:core="sap.ui.core" 
		   xmlns="sap.m" 
		   xmlns:layout="sap.ui.layout"
		   xmlns:form="sap.ui.layout.form"
		   controllerName="User.view.S2"><!--Copied from source-->

	<Page id="second" showNavButton="true" navButtonPress="_navButtonPress">
		<headerContent></headerContent>
		<content>
		<layout:Grid defaultSpan="L12 M12 S12" width="auto">
			<layout:content>
				<form:Form>
					<form:title><core:Title text="Personal Information" /></form:title>
					<form:layout>
          				<form:ResponsiveGridLayout labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1" />
          			</form:layout>
        			<form:formContainers>
        				<form:FormContainer>
							<form:FormElement label="First Name">
								<form:fields>
									<Input value="{Firstname}" />
								</form:fields>
							</form:FormElement>
							<form:FormElement label="Last Name">
								<form:fields>
									<Input value="{Lastname}" />
								</form:fields>
							</form:FormElement>
							<form:FormElement label="Email">
								<form:fields>
									<Input value="{Email}" />
								</form:fields>
							</form:FormElement>
							<form:FormElement label="Phone Number">
								<form:fields>
									<Input value="{ContactNo}" />
								</form:fields>
							</form:FormElement>
						</form:FormContainer>
					</form:formContainers>
				</form:Form>
			</layout:content>
		</layout:Grid>
		</content>
	</Page>
</core:View>